    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "drizzle-kit": "^0.31.5",
    "tsx": "^4.16.2",
    "typescript": "^5.9.2"
  }
}
